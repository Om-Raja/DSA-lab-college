;GNUsim 8085

;subtration
LDA 0000H
MOV C,A
LDA 0002H
SUB C
STA 0003H


;addition
LDA 0000H
MOV B,A
LDA 0001H
ADD B
STA 0004H

;MULTIPLICATION
LDA 0002H
MOV D,A
LDA 0003

;SWAPPING OF TWO NUMBERS
LDA 0005H
MOV E,A
LDA 0006H
STA 0005H
MOV A, E
STA 0006H

;SWAPPING OF THEREE NUMBERS
LDA 0007H
MOV B,A
LDA 0008H
MOV C,A
LDA 0009H
STA 0008H
MOV A,B
STA 0009H
MOV A,C
STA 0007H
HLT
